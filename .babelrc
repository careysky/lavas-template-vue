{
    "presets": [
        [
            "env"
        ],
        "stage-2"
    ],
    "plugins": [
        "transform-runtime",
        "add-module-exports",
        "dynamic-import-node",
        ["transform-imports", {
            "vuetify": {
                "transform": "vuetify/es5/components/${member}",
                "preventFullImport": true
            }
        }]
    ],
    "comments": false
}
